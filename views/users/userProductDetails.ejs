    <script>
      // Get references to the search input, search button, and close button
      const searchInput = document.getElementById("search_input");
      const searchForm = document.getElementById("search_form");
      const searchButton = document.getElementById("search_button");
      const closeSearch = document.getElementById("close_search");
      const searchBox = document.getElementById("search_input_box");

      // Function to open the search box
      function openSearch() {
        searchBox.style.display = "block";
      }

      // Function to close the search box
      function closeSearchBox() {
        searchBox.style.display = "none";
      }

      // Event listener for opening the search box
      searchButton.addEventListener("click", (e) => {
        e.preventDefault();
        openSearch();
      });

      // Event listener for closing the search box
      closeSearch.addEventListener("click", (e) => {
        e.preventDefault();
        closeSearchBox();
      });

      // Event listener to close search when clicking outside the search box
      document.addEventListener("click", (e) => {
        if (!searchForm.contains(e.target) && !searchBox.contains(e.target)) {
          closeSearchBox();
        }
      });
    </script>
