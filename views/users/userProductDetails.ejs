    <div class="product_image_area">
      <div class="container">
        <div class="row s_product_inner">
          <div class="col-lg-6">
            <div class="s_Product_carousel">
              <div class="single-prd-item zoom-container">
                <button class="btn btn-primary btn-prev" id="prevBtn"><</button>
                <img
                  class="img-fluid zoom-image"
                  src="/uploadProductImages/<%= product.image[0] %>"
                  alt=""
                />
                <!-- Add previous and next buttons below the image -->

                <button class="btn btn-primary btn-next" id="nextBtn">></button>
              </div>
            </div>
          </div>
          <div class="col-lg-5 offset-lg-1">
            <div class="s_product_text">
              <h3><%= product.productname %></h3>
              <h2>Rs. <%= product.price %></h2>
              <ul class="list">
                <li>
                  <a class="active" href="#"
                    ><span>Category</span> : Accessories</a
                  >
                </li>
                <a href="#">
                  <span style="color: black">Availability</span>
                  <% if (product.stock===0) { %> :
                  <span
                    style="color: red; font-size: larger; white-space: nowrap"
                    >Not in Stock</span
                  >
                  <% } else { %> :
                  <span style="color: #ff1493; font-size: larger"
                    >In Stock</span
                  >
                  <% } %>
                </a>
              </ul>
              <p><%= product.description %>.</p>

              <!-- Your existing quantity input code -->
              <div>
                <% if (product.stock > 0) { %>
                  <div class="input-group quantity mr-3" style="width: 130px; margin-bottom: 0;">
                    <div class="input-group-btn">
                      <button class="btn btn-primary btn-minus" id="dec-btn">
                        <i class="fa fa-minus"></i>
                      </button>
                    </div>
              
                    <span class="form-control bg-secondary border-0 text-center" id="inputQuantity">1</span>
              
                    <div class="input-group-btn">
                      <button class="btn btn-primary btn-plus" id="inc-btn">
                        <i class="fa fa-plus"></i>
                      </button>
                    </div>
                  </div>
              
                  <div class="card_area d-flex align-items-center" style="margin-top: 0;">
                    <button class="primary-btn" id="addtocart" href="#">
                      Add to Cart
                    </button>
                  </div>
                <% } %>
              </div>
              
            </div>
          </div>
        </div>
